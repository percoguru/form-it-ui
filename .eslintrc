{
  "parser": "@typescript-eslint/parser", 
  "parserOptions": {
    "ecmaVersion": 2020, 
    "sourceType": "module", 
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "extends": [
    "plugin:react/recommended",
    "plugin:@typescript-eslint/recommended", 
    "plugin:prettier/recommended"
  ],
  "rules": {
    "linebreak-style": 0,
    "max-len": 0,
    "camelcase": 0,
    "no-underscore-dangle": 0,
    "space-before-function-paren": 0,
    "quotes": [1, "single"]
  }
}
